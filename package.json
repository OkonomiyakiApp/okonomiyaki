    "@types/cookie": "^0.5.1",
    "@types/mocha": "^10.0.1",
    "@zerodevx/svelte-toast": "^0.9.5",
    "autoprefixer": "^10.4.15",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.6"
